<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.remi.ekio">

    <uses-feature android:name="android.hardware.camera2" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BeforePictureActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".SignInActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".GoodMatchActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".BigPhotoActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".SaveInfoActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ChooseFromCollectionActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".CollectionShowcaseActivity"
            android:screenOrientation="portrait"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>